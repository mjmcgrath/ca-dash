<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="securityPU" transaction-type="JTA">
    <jta-data-source>jdbc/sample</jta-data-source>
    <exclude-unlisted-classes>true</exclude-unlisted-classes>
    <properties/>
  </persistence-unit>
  <persistence-unit name="clinicalActivityPU" transaction-type="JTA">
    <jta-data-source>jdbc/clinicalactivity</jta-data-source>
    <class>edu.umm.radonc.ca_dash.model.Activity</class>
    <class>edu.umm.radonc.ca_dash.model.Department</class>
    <class>edu.umm.radonc.ca_dash.model.Hospital</class>
    <class>edu.umm.radonc.ca_dash.model.Procedure</class>
    <exclude-unlisted-classes>true</exclude-unlisted-classes>
    <properties/>
  </persistence-unit>
</persistence>
